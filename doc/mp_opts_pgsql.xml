<variablelist>
  <varlistentry>
    <term><option>-u</option></term>
    <term><option>--user=<replaceable>USER</replaceable></option></term>
    <listitem>
      <para>The PostgreSQL user name to use when connecting to the server.</para>
    </listitem>
  </varlistentry> 
  <varlistentry>
    <term><option>-p</option></term>
    <term><option>--password=<replaceable>PASSWORD</replaceable></option></term>
    <listitem>
      <para>The password to use when connecting to the server.</para>
    </listitem>
  </varlistentry>
  <varlistentry>
    <term><option>-d</option></term>
    <term><option>--database=<replaceable>DATABASE</replaceable></option></term>
    <listitem>
      <para>The Schema to use.</para>
    </listitem>
  </varlistentry>
  <varlistentry>
    <term><option>-p</option></term>
    <term><option>--port=<replaceable>PORT</replaceable></option></term>
    <listitem>
      <para>The TCP/IP port number or UNIX-Socketdir  to use for the connection.</para>
    </listitem>
  </varlistentry>
</variablelist>
